gen nocoal = 1 if inlist(hofuel1,-1,1,2,4,5,6,95) & inlist(hofuel2,-1,1,2,4,5,6,95) & inlist(hofuel3,-1,1,2,4,5,6,95) & inlist(hofuel4,-1,1,2,4,5,6,95) & inlist(hofuel5,-1,1,2,4,5,6,95)
