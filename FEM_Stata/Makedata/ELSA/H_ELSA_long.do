replace hh2cutil = .p if askpx1 == 1
replace hh2cutil = gsel + gas + elec + coal + para + oil + wood + oth if !mi(gsel) & !mi(gas) & !mi(elec) & !mi(coal) & !mi(para) & !mi(oil) & !mi(wood) & !mi(oth)
