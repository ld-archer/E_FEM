gen gsel = .
missing_H gseld gselb gselp gselo gsels, result(gsel)
replace gsel = gseld + gselb + gselp + gselo + gsels if !mi(gseld) & !mi(gselb) & !mi(gselp) & !mi(gselo) & !mi(gsels)

